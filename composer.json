{
    "name": "louis-dj/xml-template",
    "description": "Xml templating engine for dynamic xml string generation in php",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LouisDj\\XmlTemplate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LouisDj\\XmlTemplate\\Test\\": "tests",
            "LouisDj\\XmlTemplate\\Test\\ExampleClasses\\": "tests/example_classes/"
        }
    },
    "authors": [
        {
            "name": "Louis de Jager"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
}
